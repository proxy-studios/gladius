	<entry name="KlawOfGork" value="Griffe de Gork"/>
	<entry name="KlawOfGorkFlavor" value="<string name='Weapons/KlawFlavor'/>"/>
	<entry name="KrakGrenade" value="Grenade Krak"/>
	<entry name="KrakGrenadeFlavor" value="Les grenades Krak sont des charges implosives conçues pour craquer le blindage des véhicules."/>
	<entry name="KrakMissileLauncher" value="Lance-missile Krak"/>
	<entry name="KrakMissileLauncherFlavor" value="<string name='Weapons/BlastFlavor'/>"/>
	<entry name="KrakstormMissileSilo" value="Silo de missile Krak-tempête"/>
	<entry name="KrakstormMissileSiloFlavor" value="<string name='Weapons/BlastFlavor'/>"/>
	<entry name="KustomMegaSlugga" value="Méga-Slugga kustom"/>
	<entry name="KustomMegaSluggaFlavor" value="L'arme favorite d'un Mek devient souvent son projet préféré, bricolé jusqu'à bénéficier d'une profusion d'électro-mécano-bidules. Les bons jours, le plus portable de ces sortes de canons peut trouer un Space Marine à vingt pas. La puissance des méga armes kustom à toutefois un coût, bien sûr, et une telle arme en surchauffe est susceptible d'exploser. Néanmoins les dégâts spectaculaires occasionnés par ces armes font qu'elles valent de prendre le risque."/>
	<entry name="Lascannon" value="Canon laser"/>
	<entry name="LascannonFlavor" value="<string name='Weapons/LasFlavor'/>"/>
