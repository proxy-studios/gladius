	<entry name="TwinLinkedAutocannonFlavor" value="<string name='Weapons/AutoFlavor'/>"/>
	<entry name="TwinLinkedAssaultCannon" value="Synchronisierte Sturmkanone"/>
	<entry name="TwinLinkedAssaultCannonFlavor" value="<string name='Weapons/AutoFlavor'/>"/>
	<entry name="TwinLinkedBigShoota" value="Synchronisierte Fette Wumme"/>
	<entry name="TwinLinkedBigShootaFlavor" value="<string name='Weapons/ShootaFlavor'/>"/>
	<entry name="TwinLinkedBoltgun" value="Synchronisierter Bolter"/>
	<entry name="TwinLinkedBoltgunFlavor" value="<string name='Weapons/BoltFlavor'/>"/>
	<entry name="TwinLinkedGaussBlaster" value="Synchronisierter Gauss-Blaster"/>
	<entry name="TwinLinkedGaussBlasterFlavor" value="<string name='Weapons/GaussFlavor'/>"/>
	<entry name="TwinLinkedHeavyBolter" value="Synchronisierter Schwerer Bolter"/>
	<entry name="TwinLinkedHeavyBolterFlavor" value="<string name='Weapons/BoltFlavor'/>"/>
	<entry name="TwinLinkedHeavyPhosphorBlaster" value="Synchronisierter Schwerer Phosphorblaster"/>
	<entry name="TwinLinkedHeavyPhosphorBlasterFlavor" value="<string name='Weapons/PhosphorFlavor'/>"/>
	<entry name="TwinLinkedHydraAutocannon" value="Synchronisierte Hydra-Maschinenkanone"/>
	<entry name="TwinLinkedHydraAutocannonFlavor" value="Die Hydra-Maschinenkanone ist ein übergroßes Geschütz mit langem Lauf auf einem Flugabwehrfahrzeug. Ihre tödlichen Geschosse zischen durch den Himmel, gelenkt von einem vorausberechnenden Logikgeist."/>
	<entry name="TwinLinkedLascannon" value="Synchronisierte Laserkanone"/>
	<entry name="TwinLinkedLascannonFlavor" value="<string name='Weapons/LasFlavor'/>"/>
	<entry name="TwinLinkedParticleBeamer" value="Synchronisierter Partikelstrahler"/>
	<entry name="TwinLinkedParticleBeamerFlavor" value="<string name='Weapons/ParticleFlavor'/>"/>
	<entry name="TwinLinkedShoota" value="Synchronisierte Wumme"/>
	<entry name="TwinLinkedShootaFlavor" value="<string name='Weapons/ShootaFlavor'/>"/>
	<entry name="TwinLinkedSupaShoota" value="Synchronisierte Supawumme"/>
	<entry name="TwinLinkedSupaShootaFlavor" value="<string name='Weapons/ShootaFlavor'/>"/>
	<entry name="TwinLinkedTeslaDestructor" value="Synchronisierter Tesla-Annihilator"/>
	<entry name="TwinLinkedTeslaDestructorFlavor" value="<string name='Weapons/TeslaFlavor'/>"/>
	<entry name="UrtySyringe" value="Beruhigungzspritzä"/>
	<entry name="UrtySyringeFlavor" value="Die zähe Masse, die Painboyz anderen als Gerinnungsmittel injizieren, hat für die nicht-orkoide Physiologie tödliche Folgen. Pilzbefall verstopft die Arterien und die Lunge verwandelt sich in einen Bovist voller Sporen. Das Opfer kollabiert daraufhin schnell und zurück bleibt ein lebloser Körper, aus dessen Öffnungen dichter Schaum austritt."/>
	<entry name="Voidblade" value="Entropieklinge"/>
	<entry name="VoidbladeFlavor" value="Die glänzende schwarze Schneide einer Entropieklinge blitzt im Diesseits sowie jenseits davon. Bei Berührung löst sie sofort die Molekularstruktur jeglicher Materie auf."/>
	<entry name="WarpBlastBurst" value="Warpblitzexplosion"/>
	<entry name="WarpBlastLance" value="Warpblitzlanze"/>
	<entry name="Warscythe" value="Kriegssense"/>
	<entry name="WarscytheFlavor" value="Kriegssensen sind energiegeladene Klingenwaffen und gelten seit Jahrtausenden als die bevorzugte Waffe der Necron Lords und ihrer Leibwächter. Aufgrund ihres hohen Gewichts und ihrer Sperrigkeit können sie in den Händen eines weniger hochentwickelten Wesens nur wenig ausrichten. Werden sie jedoch mit der unermüdlichen mechanischen Muskelkraft eines Necrons geschwungen, sind sie äußerst wirkungsvoll."/>
	<entry name="WeirdboyStaff" value="Weirdboy-Stab"/>
	<entry name="WeirdboyStaffFlavor" value="Die glänzenden Zauberanhänger und klirrenden Glocken, die einen Weirdboy-Stab zieren, täuschen über die schreckliche Wirkung hinweg, die diese Waffe auf dem Schlachtfeld erzielen kann. Dank der Kupferstange zur Erdung kann der Träger die ungezügelte Waaagh-Energie im Zaum halten. Mit jedem Treffer, den der Weirdboy landet, entlädt sich der Stab mit einem grünen Funkenregen."/>
	<entry name="WireWeed" value="<string name='Features/WireWeed'/>"/>
	<entry name="WireWeedFlavor" value="<string name='Features/WireWeedFlavor'/>"/>
	<entry name="ZzapGun" value="Zzapwumme"/>
	<entry name="ZzapGunFlavor" value="Zzapwummen feuern instabile Blitze ab und sind dazu in der Lage, mit einem knisternden Funkenregen selbst die dickste Panzerung von feindlichen Fahrzeugen zu durchbrechen. Allerdings neigen sie zu Überhitzung und können den Schützen mit einem Stromschlag töten."/>
</language>